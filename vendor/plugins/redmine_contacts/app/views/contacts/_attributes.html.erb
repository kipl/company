<div id="attributes">


<%- if Gem.available?('vpim') -%>

<div class="contextual">
	<%= link_to 'vCard', params.merge({:format => 'vcf'}), :class => 'vcf' %>
</div>               
<%- end -%>
<h3><%= if !@contact.is_company then l(:label_contact) else l(:label_company) end %></h3>  

